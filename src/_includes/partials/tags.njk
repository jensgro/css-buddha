    <div class="tags">
      <ul class="taglist">
        {%- for tag in tags -%}
        {% set tagUrl %}/tags/{{ tag }}/{% endset %}
        <li class="taglist__item">
          <a class="taglist__link" href="{{ tagUrl | url }}">
            {{ tag }}
          </a>
        </li>
        {%- endfor -%}
      </ul>
    </div>
